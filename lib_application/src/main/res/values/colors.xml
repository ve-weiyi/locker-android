<!--    colorPrimary：您的应用的主要品牌颜色，主要用于主题-->
    <color name="colorPrimary">@color/blue</color>
<!--    colorPrimaryVariant：您的主要品牌颜色的较浅/较暗变体，在主题中很少使用-->
    <color name="colorPrimaryVariant">#512DA8</color>
<!--colorOnPrimary：用于显示在原色上方的元素的颜色（例如，文本和图标，根据可访问性，通常为白色或半透明的黑色）-->
    <color name="colorOnPrimary">@color/white</color>
<!--    colorSecondary：您应用程式的次要品牌色彩，主要用于强调某些需要突出的小部件-->
    <color name="colorSecondary">@color/light_blue</color>
<!--    colorSecondaryVariant：您的次要品牌颜色的较浅/较深变体，在主题中很少使用-->
    <color name="colorSecondaryVariant">#0039CB</color>
<!--    colorOnSecondary：用于显示在辅助颜色顶部的元素的颜色-->
    <color name="colorOnSecondary">@color/white</color>
<!--    colorError：用于错误的颜色（通常为红色阴影）-->
    <color name="colorError">#F44336</color>
<!--    colorOnError：用于显示在错误颜色顶部的元素的颜色-->
    <color name="colorOnError">#FFFFFF</color>
<!--    colorSurface：(夜间模式标题栏颜色)用于表面的颜色（即材料“纸张”）-->
    <color name="colorSurface">@color/black</color>
<!--    colorOnSurface：用于显示在表面颜色顶部的元素的颜色-->
    <color name="colorOnSurface">#212121</color>
<!--    android:colorBackground：所有其他屏幕内容后面的颜色(view默认背景颜色)-->
    <color name="colorBackground">@color/grey100</color>
<!--    colorOnBackground：用于显示在背景色上方的元素的颜色()-->
    <color name="colorOnBackground">#212121</color>


